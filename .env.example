# MCP Server Environment Variables
# Copy this file to .env and modify as needed

# Server configuration
MCP_HOST=0.0.0.0
MCP_PORT=8000
MCP_DEBUG=False

# Security
MCP_SECRET_KEY=your-secret-key-here
MCP_API_KEYS=key1,key2,key3

# SkySQL API
SKYSQL_API_KEY=your-skysql-api-key-here

# CORS settings
MCP_CORS_ORIGINS=http://localhost,http://localhost:3000

# Database (uncomment if needed)
# MCP_DATABASE_URL=postgresql://user:password@localhost/dbname

# Logging
MCP_LOG_LEVEL=INFO

# Agent settings
MCP_MAX_AGENTS=10
MCP_AGENT_TIMEOUT=60 